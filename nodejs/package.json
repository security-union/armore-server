{
    "name": "armore-server",
    "version": "0.1.0",
    "description": "Best backend server of the world.",
    "repository": {
        "type": "git"
    },
    "license": "MIT",
    "author": "Dario Talarico",
    "scripts": {
        "build": "npm run build-ts && npm run tslint",
        "build-ts": "tsc",
        "debug": "npm run build && npm run watch-debug",
        "prettier": "prettier \"**/*.{json,md,js,ts}\" --write",
        "serve": "node dist/start.js",
        "serve-debug": "nodemon --inspect dist/start.js",
        "start": "npm run serve",
        "test": "jest --forceExit --coverage --verbose",
        "test-backend": "yarn install && yarn test --runInBand --detectOpenHandles",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "watch-node": "nodemon --config nodemon.json dist/start.js",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w",
        "start-ws-client": "node dist/ws_gateway/client.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "@google-cloud/storage": "4.1.3",
        "@sendgrid/mail": "6.4.0",
        "@slack/webhook": "^5.0.3",
        "@types/flatbuffers": "1.9.1",
        "amqplib": "0.5.3",
        "bluebird": "3.7.1",
        "body-parser": "1.18.2",
        "errorhandler": "1.5.0",
        "express": "4.17.1",
        "express-validator": "6.3.1",
        "flatbuffers": "1.11.0",
        "get-port": "5.0.0",
        "jsonwebtoken": "8.5.1",
        "lodash": "4.17.15",
        "moment": "2.24.0",
        "node-pushnotifications": "1.2.0",
        "pg": "7.11.0",
        "phone": "^2.4.17",
        "request": "2.88.0",
        "request-promise": "4.2.2",
        "sinon": "7.3.2",
        "swagger-ui-express": "4.0.7",
        "tedis": "0.1.9",
        "twilio": "^3.50.0",
        "uuid": "3.3.2",
        "winston": "3.2.1",
        "ws": "6.0.0",
        "yamljs": "0.3.0"
    },
    "devDependencies": {
        "@types/amqplib": "0.5.3",
        "@types/bluebird": "3.5.29",
        "@types/body-parser": "1.16.8",
        "@types/errorhandler": "0.0.32",
        "@types/express": "4.17.0",
        "@types/express-validator": "3.0.0",
        "@types/get-port": "4.2.0",
        "@types/jest": "24.0.23",
        "@types/jsonwebtoken": "8.3.2",
        "@types/lodash": "4.14.149",
        "@types/node": "12.12.25",
        "@types/node-pushnotifications": "1.0.3",
        "@types/pg": "7.4.14",
        "@types/phone": "^2.4.0",
        "@types/request": "2.48.4",
        "@types/shelljs": "0.7.8",
        "@types/sinon": "7.0.12",
        "@types/supertest": "2.0.4",
        "@types/swagger-ui-express": "3.0.1",
        "@types/uuid": "3.4.4",
        "@types/winston": "2.4.4",
        "@types/ws": "6.0.1",
        "@types/yamljs": "0.2.30",
        "chai": "4.1.2",
        "concurrently": "3.5.1",
        "husky": "2.3.0",
        "jest": "26.6.3",
        "nodemon": "1.18.10",
        "prettier": "2.0.2",
        "pretty-quick": "1.10.0",
        "supertest": "3.0.0",
        "ts-jest": "26.4.4",
        "ts-node": "5.0.0",
        "tslint": "5.9.1",
        "typescript": "3.8.3"
    }
}
